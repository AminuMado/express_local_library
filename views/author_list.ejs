<!DOCTYPE html>
<html>
  <%- include('./partials/head.ejs') %>
  <body>
    <div class="container-fluid">
      <div class="row">
        <%- include('./partials/nav.ejs') %>
        <div class="col-sm-10">
            <h1><%- title %></h1>
            <% if (author_list) { %>
            <ul>
              <% author_list.forEach(author => { %>
              <li>
                <a href=<%- author.url %> > <%- author.name %></a>
                (<%- author.lifespan %>)
              </li>
              <% }) %>
            </ul>
            <% } else { %>
            <p>There are no authors.</p>
            <% } %> 
        </div>
      </div>
    </div>
  </body>
</html>

 